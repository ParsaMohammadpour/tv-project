<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AddChannelController">
   <children>
      <Button fx:id="backButton" layoutX="45.0" layoutY="619.0" mnemonicParsing="false" onMouseClicked="#back" onMouseEntered="#backEnter" onMouseExited="#backExit" text="Back" textFill="#1e0097" />
      <TextField fx:id="channelNameTextField" layoutX="256.0" layoutY="159.0" promptText="Channel Name">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </TextField>
      <Button fx:id="selectManagerButton" layoutX="151.0" layoutY="268.0" mnemonicParsing="false" onMouseClicked="#selectManager" onMouseEntered="#selectEnter" onMouseExited="#selectExit" text="Select Manager" />
      <ListView fx:id="managerListView" layoutX="250.0" layoutY="237.0" prefHeight="200.0" prefWidth="200.0" visible="false" />
      <Button fx:id="makeButton" layoutX="324.0" layoutY="553.0" mnemonicParsing="false" onMouseClicked="#make" onMouseEntered="#makeEnter" onMouseExited="#makeExit" text="Make" textFill="#00a108" />
      <Label fx:id="channelNameLabel" layoutX="284.0" layoutY="189.0" text="This Name Has Been Taken" textFill="RED" visible="false" />
   </children>
</AnchorPane>
